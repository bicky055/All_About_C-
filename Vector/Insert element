#include <iostream>
#include<vector>
using namespace std;
int main() {
    
    vector<int> v;//Dynamic arr initilization
    
    v.push_back(1);
    v.push_back(2);
    v.push_back(3);
    v.push_back(4);
    //print arr elements
    for(int i=0;i<v.size();i++)
    {
        cout<<v[i]<<" ";
    }cout<<"\n";
    //another method to print arr elements
    vector<int>::iterator it;
    for(it=v.begin();it!=v.end();it++)
    {
        cout<<*it<<" ";
    }cout<<"\n";
    //3  method 
    for(auto element:v)
    {
        cout<<element<<" ";
    }cout<<endl;
    
   return 0;
}


